{
  "name": "aspire-education",
  "version": "1.0.0",
  "description": "This is a great app if you want to study math and language",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=logo.png --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=logo.png --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Aspire Education\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=logo.png --prune=true --out=release-builds"
  },
  "author": "Keegan Albert",
  "license": "MIT",
  "devDependencies": {
    "electron": "^18.3.0",
    "electron-reloader": "^1.2.3"
  },
  "dependencies": {
    "electron-store": "^8.0.2"
  }
}
